          deck[i]=new Card (13, (char)'\u2764');
          deck[i+13]=new Card (13, (char)'\u2666');
          deck[i+26]=new Card (13, (char)'\u2663');
          deck[i+39]=new Card (13, (char)'\u2660');
        }
        else{
          deck[i]=new Card (i, (char)'\u2764');
          deck[i+13]=new Card (i, (char)'\u2666');
          deck[i+26]=new Card (i, (char)'\u2663');
          deck[i+39]=new Card (i, (char)'\u2660');
        }
      }
      
      //shuffle deck
      for (int j=52;j>0;j--){
        int randomIndex = generator.nextInt( j );
        Card dummy=deck[randomIndex];
        deck[randomIndex]=deck[j-1];
        deck[j-1]=dummy;     
      }
      StdDraw.clear(StdDraw.LIGHT_GRAY);
      displayScore(humanScore,computerScore);
      StdDraw.setPenColor();
      if(dealer){
        StdDraw.text(600, 70, "dealer");
      }
      else{
        StdDraw.text(600, 530, "dealer");
      }
      
      //Prompts player to discrad two cards, 
      boolean select1 =false;
      boolean select2=false;
      //humanHand
      for(int i=0; i<6; i++){
        Card nextCard=deck[i];
        StdDraw.setPenColor(StdDraw.WHITE);
        StdDraw.filledRectangle(100+200*i,200,57,89);
        outlineRectangle(100+200*i,200,57,89);
        StdDraw.setPenColor();
        StdDraw.line(100+200*i-57, 200-89, 100+200*i+57, 200-89);
        StdDraw.text(100+200*i,200,nextCard.toString());
      }
             StdDraw.show(0);
      Card thrown1 = new Card(0,(char)'x');
      Card thrown2 = new Card(0,(char)'x');
      while(!(select1&&select2)){
        if(StdDraw.mousePressed()){
          if((StdDraw.mouseY()>200-89)&&(StdDraw.mouseY()<200+89)){
            for(int i=0; i<6; i++){
              if((StdDraw.mouseX()>100+200*i-57)&&(StdDraw.mouseX()<100+200*i+57)){
                if(!select1){
                  select1=true;
                  thrown1=deck[i];
                  StdDraw.setPenColor(StdDraw.GREEN);
                  StdDraw.filledRectangle(100+200*i, 200, 60, 92);
                  StdDraw.setPenColor(StdDraw.WHITE);
                  StdDraw.filledRectangle(100+200*i, 200, 57, 89);
                  StdDraw.text(100+200*i, 200, thrown1.toString());
                  StdDraw.show(0);
                }
                else{
                  if(!thrown1.equals(deck[i])){
                    thrown2=deck[i];
                    select2=true;
                  }
                }
              }
            }
          }
        }
      }
      while(StdDraw.mousePressed()){
      }
      crib[0]=thrown1;
      crib[1]=thrown2;
      int humanUnplayedIndex=0;
      for (int i=0; i<6; i++){
        if((!cardEquals(deck[i],thrown1))&&(!cardEquals(deck[i],thrown2))){
          humanHand[humanUnplayedIndex]=deck[i];
          humanUnplayedIndex++;
        }
      }
      Card[] computerDealt = new Card[6];
      for (int i=0; i<6; i++){
        Card nextCard=deck[i+6];
        computerDealt[i]=nextCard;
      }
      
      //Decides what to throw
      computerThrow=toCrib(computerDealt,!dealer);
      crib[2]=deck[computerThrow[0]+6];
      crib[3]=deck[computerThrow[1]+6];
      int computerUnplayedIndex=0;
      for (int i=0; i<6; i++){
        if ((i!=computerThrow[0])&&(i!=computerThrow[1])){
          computerHand[computerUnplayedIndex]=deck[i+6];
          computerUnplayedIndex++;
        }
      }
      Card cut=deck[12];
      //distinguish between hand and unplayed cards for pegging purposes
      StdDraw.clear(StdDraw.LIGHT_GRAY);
      displayCut(cut);
      displayScore(humanScore,computerScore);
      humanHand[4]=cut;
      crib[4]=cut;
      computerHand[4]=cut;
           StdDraw.show(0);
      //two points for the Jack
      if(cut.findRank()==11){
        if(dealer){
          humanScore=humanScore+2;
          displayScore(humanScore, computerScore);
          if(humanScore>=121){
            gameOver=true;
          }
        }
        else{
          computerScore=computerScore+2;
          displayScore(humanScore, computerScore);
          if(computerScore>=121){
            gameOver=true;
          }
        }
      }
      
      //setting up for Pegging
      ArrayList<Card> cardsInPlay=new ArrayList<Card>();
      boolean[] humanCardsInHand=new boolean[4];
      boolean[] computerCardsInHand=new boolean[4];
      for(int i=0;i<4;i++){
        humanCardsInHand[i]=true;
        computerCardsInHand[i]=true;
      }
      boolean pegging=true;
      boolean humanGo=false;
      boolean computerGo=false;
      int computerCardsRemaining=4;
      int humanCardsRemaining=4;
      int count=0;
      boolean delay=false;
      boolean computerPegsFirst=dealer;
      displayCardsInHand(humanCardsInHand, humanHand);
      //pegging process
      while (pegging&&!gameOver){
        if(!computerPegsFirst){
          Card played=new Card(0,(char)'x');
          displayScore(humanScore,computerScore);
          displayCount(count);
          humanGo=true;
          int limit=31-count;
          for(int i=0;i<4;i++){
            if(humanCardsInHand[i]){
              if(humanHand[i].findValue()<=limit){
                humanGo=false;
              }
            }
          }
          //if not a go, plays a card
          if(!humanGo){
            boolean legalPlay=false;
            //makes sure play doesn't go over count of 31
            while (!legalPlay){
              boolean pressed=false;
              while(!pressed){
                if(StdDraw.mousePressed()){
                  if((StdDraw.mouseY()>100-89)&&(StdDraw.mouseY()<100+89)){
                    for(int i=0; i<4; i++){
                      if((StdDraw.mouseX()>150+200*i-57)&&(StdDraw.mouseX()<150+200*i+57)){
                        if(humanCardsInHand[i]){
                          played=humanHand[i];
                          pressed=true;
                        }
                      }
                    }
                  }
                }
              }
              while(StdDraw.mousePressed()){
              }
                if(played.findValue()+count<=31){ 
                  legalPlay=true;
                }
            }
            for(int i=0;i<4;i++){
              if ((cardEquals(played,humanHand[i]))&&(humanCardsInHand[i])){
                cardsInPlay.add(played);
                displayCardsInPlay(cardsInPlay);
                seenCardRanks.add(played.findRank());
                for (int j=seenCardRanks.size()-1; j>0; j--){
                  if (seenCardRanks.get(j)<seenCardRanks.get(j-1)){
                    int dummy=seenCardRanks.get(j);
                    seenCardRanks.remove(j);
                    seenCardRanks.add(j-1,dummy);
                  }
                }
                //     validPlay=true;
                humanCardsRemaining--;
                int points=scorechecker(cardsInPlay,count);
                humanScore=humanScore+points;
                displayScore(humanScore, computerScore);
                
                if(humanScore>=121){
                  gameOver=true;
                }
                humanCardsInHand[i]=false;
              }
            }
            count=count+played.findValue();
            displayCount(count);
            displayCardsInHand(humanCardsInHand, humanHand);
            displayCardsInPlay(cardsInPlay);
            StdDraw.show(0);
          }
          else{
            waitForClick();
          }
          if (computerGo&&humanGo){
            cardsInPlay.clear();
            displayCardsInPlay(cardsInPlay);
            count=0;
            humanScore++;
            if(humanCardsRemaining+computerCardsRemaining>0){
              humanGo=false;
              computerGo=false;
            }
            else{
              pegging=false;
            }
            if(humanScore>=121){
              gameOver=true;
            }
          }
        }
        computerPegsFirst=false;
        if(!gameOver){
            computerGo=true;
              for(int i=0;i<4;i++){
                int limit=31-count;
                if(computerCardsInHand[i]){
                  if(computerHand[i].findValue()<=limit){
                    computerGo=false;
                  }
                }
              }
            if(!computerGo){
              
              for(int i=1; i<14; i++){
                for(int j=1; j<14; j++){
                  for(int k=1; k<14; k++){
                    for(int l=1; l<14; l++){
                      tablePossible[i][j][k][l]=false;
                    }
                  }
                }
              }
              Card playedCard=new Card(0,(char)'x');
              int[] fictitiousRanks=new int[4];
              for(int k=0; k<seenCardRanks.size(); k++){
                fictitiousRanks[k]=seenCardRanks.get(k);
              }
              for (int q=0; q<Math.pow(13,4-seenCardRanks.size()); q++){
                int temp=q;
                for(int l=seenCardRanks.size(); l<4; l++){
                  fictitiousRanks[l]=temp%13;
                  temp=(temp-temp%13)/13;
                  if(fictitiousRanks[l]==0){
                    fictitiousRanks[l]=13;
                  }
                }
                int [] sortedFictitiousRanks=new int[4];
                for (int n=0; n<4; n++){
                  sortedFictitiousRanks[n]=fictitiousRanks[n];
                }
                for (int n=0; n<3; n++){
                  for (int p=0; p<3-n; p++){
                    if (sortedFictitiousRanks[p]>sortedFictitiousRanks[p+1]){
                      int dummy=sortedFictitiousRanks[p];
                      sortedFictitiousRanks[p]=sortedFictitiousRanks[p+1];
                      sortedFictitiousRanks[p+1]=dummy;
                    }
                  }
                }
                tablePossible[sortedFictitiousRanks[0]][sortedFictitiousRanks[1]][sortedFictitiousRanks[2]][sortedFictitiousRanks[3]]=true;
              }
              int[] scoreSum=new int[4];
              for(int i=0; i<4; i++){
                scoreSum[i]=0;
              }
              for(int i=1; i<14; i++){
                for(int j=1; j<14; j++){
                  for(int k=1; k<14; k++){
                    for(int l=1; l<14; l++){
                      if(tablePossible[i][j][k][l]){
                        boolean[] fakeInHand=new boolean[4];
                        for(int m=0; m<4; m++){
                          fakeInHand[m]=true;
                        }
                        for(int m=0; m<seenCardRanks.size();m++){
                          int seenSize=seenCardRanks.size();
                          if(seenCardRanks.get(m)==i){
                            fakeInHand[0]=false;
                            m++;
                          }
                          if(m==seenSize){
                            break;
                          }
                          if(seenCardRanks.get(m)==j){
                            fakeInHand[1]=false;
                            m++;
                          }
                          if(m==seenSize){
                            break;
                          }
                          if(seenCardRanks.get(m)==k){
                            fakeInHand[2]=false;
                            m++;
                          }
                          if(m==seenSize){
                            break;
                          }
                          if(seenCardRanks.get(m)==l){
                            fakeInHand[3]=false;
                            m++;
                          }
                        } 
                        Card[] fakeHand= new Card[4];
                        fakeHand[0]=new Card(i,(char)'\u2764');
                        fakeHand[1]=new Card(j,(char)'\u2666');
                        fakeHand[2]=new Card(k,(char)'\u2663');
                        fakeHand[3]=new Card(l,(char)'\u2660');
                        for(int m=0; m<4; m++){
                          int score=0;
                          if(computerCardsInHand[m]){
                            if(computerHand[m].findValue()+count<=31){
                              cardsInPlay.add(computerHand[m]);
                              computerCardsInHand[m]=false;
                              computerCardsRemaining--;
                              score=scorechecker(cardsInPlay,count);
                              count=count+computerHand[m].findValue();
                              if(dealer){
                                score=dealerHand[i][j][k][l]*(score-pegging( humanCardsRemaining,fakeInHand, fakeHand,computerCardsRemaining,computerCardsInHand,computerHand,cardsInPlay,count));
                              }
                              else{
                                score=poneHand[i][j][k][l]*(score-pegging( humanCardsRemaining,fakeInHand, fakeHand,computerCardsRemaining,computerCardsInHand,computerHand,cardsInPlay,count));
                              }
                              scoreSum[m]=scoreSum[m]+score;
                              count=count-computerHand[m].findValue();
                              computerCardsRemaining++;
                              computerCardsInHand[m]=true;
                              cardsInPlay.remove(cardsInPlay.size()-1);
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
              int maxScore=-10000000;
              for (int i=0; i<4; i++){
                if(computerCardsInHand[i]){
                  if(computerHand[i].findValue()+count<=31){
                    if(scoreSum[i]>maxScore){
                      maxScore=scoreSum[i];
                      playedCard=computerHand[i];
                    }
                  }
                }
              }
              for (int i=0; i<4; i++){
                if (cardEquals(playedCard, computerHand[i])){
                  computerCardsInHand[i]=false;
                }
              }
              humanGo=false; //multiple last cards by computer
              cardsInPlay.add(playedCard);
              displayCardsInPlay(cardsInPlay);
              StdDraw.show(0);
              int points=scorechecker(cardsInPlay,count);
              count=count+playedCard.findValue();
              computerScore=computerScore+points;
              displayScore(humanScore, computerScore);
              if(computerScore>=121){
                gameOver=true;
              }
              computerCardsRemaining--;
            }
        }
        displayCount(count);
        displayCardsInPlay(cardsInPlay);
        StdDraw.show(0);
        if((humanGo&&computerGo)&&!gameOver){
          cardsInPlay.clear();
          displayCardsInPlay(cardsInPlay);
          count=0;
          displayCount(count);
          if(pegging){
            computerScore++;
          }
          if(humanCardsRemaining+computerCardsRemaining>0){
            humanGo=false;
            computerGo=false;
          }
          else{
            pegging=false;
          }
        }
        if((humanCardsRemaining+computerCardsRemaining==0)&&(humanGo&&computerGo)){
          pegging=false;
        }
      }
      //    StdDraw.show(0);
      StdDraw.clear(StdDraw.LIGHT_GRAY);
      displayScore(humanScore,computerScore);
      if(!gameOver){
        
        if(dealer){
          displayHand(computerHand,500);
          computerScore=computerScore+score(computerHand, false);
          displayScore(humanScore, computerScore);
//          StdDraw.show(0);
          if(computerScore>=121){
            gameOver=true;
          }
          if(!gameOver){
            displayHand(humanHand, 100);
            humanScore=humanScore+score(humanHand, false);
            displayScore(humanScore, computerScore);
            //          StdDraw.show(0);
            if(humanScore>=121){
              gameOver=true;
            }
          }
        }
        else{
          displayHand(humanHand,100);
          humanScore=humanScore+score(humanHand, false);
          displayScore(humanScore, computerScore);
//          StdDraw.show(0);
          if(humanScore>=121){
            gameOver=true;
          }
          
          if(!gameOver){
            displayHand(computerHand,500);
            computerScore=computerScore+score(computerHand, false);
            displayScore(humanScore, computerScore);
            //       StdDraw.show(0);
            if(computerScore>=121){
              gameOver=true;
            }
          }
        }
        if(!gameOver){//crib
          displayHand(crib,300);
          if(dealer){
            humanScore=humanScore+score(crib, true);
            displayScore(humanScore, computerScore);
            //           StdDraw.show(0);
            if(humanScore>=121){
              gameOver=true;
            }
          }
          else{
            computerScore=computerScore+score(crib, true);
            displayScore(humanScore, computerScore);
//            StdDraw.show(0);
            if(computerScore>=121){
              gameOver=true;
            }
          }
        }
        if(!gameOver){
          seenCardRanks.clear();
          displayScore(humanScore,computerScore);
//          StdDraw.show(0);
        }
      }
    }
    input.close();
  }
}
