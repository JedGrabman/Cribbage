import java.util.Random;
import java.util.Scanner;
import java.util.ArrayList; 

public class Cribbage{
  static public class Card{
    private final int rank;
    private final char suit;
    public Card(int cardRank, char cardSuit){
      rank=cardRank;
      suit=cardSuit;
    }
    public int findRank(){
      return(rank);
    }
    public int findValue(){
      if (findRank()>10){
        return 10;
      }
      else{
        return findRank();
      }
    }
    public char findSuit(){
      return(suit);
    }
    public String toString(){
      if (suit==((char)'\u2764')||suit==((char)'\u2666')){
        StdDraw.setPenColor(StdDraw.RED);
      }
      else{
        StdDraw.setPenColor();
      }
      String rankShortcut="";
      if((rank==13)||(rank==1)||(rank==11)||(rank==12)){
        if(rank==1){
          rankShortcut="A";
        }
        if(rank==11){
          rankShortcut="J";
        }
        if(rank==12){
          rankShortcut="Q";
        }
        if(rank==13){
          rankShortcut="K";
        }
        return(rankShortcut+ suit);
      }
      else{
        return(rank +""+ suit);
      }
    }
  }
  static public void waitForClick(){
    boolean clicked=false;
    while(!clicked){
      if(StdDraw.mousePressed()){
        while(!clicked){
          if(!StdDraw.mousePressed()){
            clicked=true;
          }
        }
      }
    }
  }
  static public void displayHand( Card[] hand, int displayHeight){
    for(int i=0; i<4; i++){
      Card nextCard=hand[i];
      StdDraw.setPenColor(StdDraw.WHITE);
      StdDraw.filledRectangle(150+200*i,displayHeight,57,89);
      outlineRectangle(150+200*i,displayHeight,57,89);
      StdDraw.setPenColor();
      StdDraw.text(150+200*i,displayHeight,nextCard.toString());
    }
    StdDraw.setPenColor(StdDraw.WHITE);
    StdDraw.filledRectangle(1100, 300, 57, 89);
    outlineRectangle(1100, 300, 57, 89);
    StdDraw.setPenColor();
    StdDraw.text(1100, 300, hand[4].toString());
    StdDraw.show(0);
    waitForClick();
  }
  static public void displayScore(int humanScore, int computerScore){
    StdDraw.setPenColor(StdDraw.LIGHT_GRAY);
    StdDraw.filledRectangle(50,550,20,20);
    StdDraw.filledRectangle(1150,550,20,20);
    StdDraw.setPenColor(StdDraw.RED);
    StdDraw.text(50, 570, "You");
    StdDraw.text(50, 550, Integer.toString(humanScore));
    StdDraw.setPenColor(StdDraw.BLUE);
    StdDraw.text(1150, 570, "Computer");
    StdDraw.text(1150, 550, Integer.toString(computerScore));
    StdDraw.show(0);
  }
    static public void displayCount(int count){
    StdDraw.setPenColor(StdDraw.LIGHT_GRAY);
    StdDraw.filledRectangle(600,550,20,20);
    StdDraw.setPenColor();
    StdDraw.text(600, 570, "Count");
    StdDraw.text(600, 550, Integer.toString(count));
  }
  static public void displayCardsInHand(boolean[] cardsInHand, Card[] hand){
    StdDraw.setPenColor(StdDraw.LIGHT_GRAY);
    StdDraw.filledRectangle(600,100,600,100);
    for(int i=0; i<4; i++){
      if(cardsInHand[i]){
        Card nextCard=hand[i];
        StdDraw.setPenColor(StdDraw.WHITE);
        StdDraw.filledRectangle(150+200*i,100,57,89);
        outlineRectangle(150+200*i,100,57,89);
        StdDraw.setPenColor();
        StdDraw.text(150+200*i,100,nextCard.toString());
      }
    }
    StdDraw.show(0);
  }
  static public void displayCardsInPlay(ArrayList<Card> cardsInPlay){
    StdDraw.setPenColor(StdDraw.LIGHT_GRAY);
    StdDraw.filledRectangle(550,300,550,100);
    for(int i=0; i<cardsInPlay.size(); i++){
      StdDraw.setPenColor(StdDraw.WHITE);
      StdDraw.filledRectangle(75+130*i, 300, 57, 89);
      outlineRectangle(75+130*i, 300, 57, 89);
      StdDraw.setPenColor();
      StdDraw.text(75+130*i, 300, cardsInPlay.get(i).toString());
    }
    StdDraw.show(0);
  }
  static public void displayCut(Card cut){
    StdDraw.setPenColor(StdDraw.WHITE);
    StdDraw.filledRectangle(1170, 300, 57, 89);
    outlineRectangle(1170, 300, 57, 89);
    StdDraw.setPenColor();
    StdDraw.text(1170, 300, cut.toString());
    StdDraw.show(0);
  }
  public static Boolean cardEquals(Card card1, Card card2){
    if((card1.findRank()==card2.findRank())&&((card1.findSuit())==(card2.findSuit()))){
      return true;
    }
    else{
      return false;
    }
  }
  static public void outlineRectangle(double widthCenter, double heightCenter, double halfWidth, double halfHeight){
    StdDraw.setPenColor();
    StdDraw.line(widthCenter-halfWidth, heightCenter-halfHeight, widthCenter+halfWidth, heightCenter-halfHeight);
    StdDraw.line(widthCenter-halfWidth, heightCenter+halfHeight, widthCenter+halfWidth, heightCenter+halfHeight);
    StdDraw.line(widthCenter-halfWidth, heightCenter-halfHeight, widthCenter-halfWidth, heightCenter+halfHeight);
    StdDraw.line(widthCenter+halfWidth, heightCenter-halfHeight, widthCenter+halfWidth, heightCenter+halfHeight);
  }
  public static int pegging(int cardsRemaining, boolean[] cardsInHand, Card[] hand, int opponentCardsRemaining, boolean[] cardsInOtherHand, Card[] otherHand, ArrayList<Card> cardsInPlay, int count){
    
    int score=0;
    int[] handValues=new int[4];
    for(int i=0;i<4;i++){
      handValues[i]=hand[i].findValue();
    }
    int maxScore=-200;
    if(count==31){
      ArrayList<Card> empty=new ArrayList<Card>();
      if(cardsRemaining>0){
        return pegging(cardsRemaining, cardsInHand, hand, opponentCardsRemaining, cardsInOtherHand, otherHand, empty, 0);
      }
      else{
        if(opponentCardsRemaining>0){
          return -pegging(opponentCardsRemaining, cardsInOtherHand, otherHand, cardsRemaining, cardsInHand, hand, empty, 0);
        }
        else{
          return 0;
        }
      }
    }
    if(cardsRemaining>0){
      for(int i=0;i<4;i++){
        if(cardsInHand[i]){
          if(count+handValues[i]<32){
            cardsInHand[i]=false;
            cardsInPlay.add(hand[i]);
            score=scorechecker(cardsInPlay, count);
            count=count+handValues[i];
            score=score-pegging(opponentCardsRemaining,cardsInOtherHand, otherHand, cardsRemaining-1, cardsInHand, hand, cardsInPlay, count);
            cardsInHand[i]=true;
            cardsInPlay.remove(cardsInPlay.size()-1);
            count=count-handValues[i];
            if (score>maxScore){
              maxScore=score;
            }
          }
        }
        
      }
      if(maxScore<-150){
        int limit=31-count;
        boolean go=true;
        for(int i=0; i<4;i++){
          if(cardsInOtherHand[i]){
            if (otherHand[i].findValue()<=limit){
              go=false;
            }
          }
        }
        if(go){
          ArrayList<Card> empty=new ArrayList<Card>();
          return -1+pegging(cardsRemaining, cardsInHand, hand, opponentCardsRemaining, cardsInOtherHand,otherHand, empty, 0);
        }
        else{
          return -pegging(opponentCardsRemaining, cardsInOtherHand, otherHand, cardsRemaining, cardsInHand, hand, cardsInPlay, count);
        }
      }
      else{
        return maxScore;
      }
    }
    else{
      if(opponentCardsRemaining>0){
        return -pegging(opponentCardsRemaining, cardsInOtherHand, otherHand, cardsRemaining, cardsInHand, hand, cardsInPlay, count);
      }
      else{
        return -1;
      }
    }
  }
  
  public static int score(Card[] cards, boolean crib){
    int score=0;
    Card[] hand=new Card [5];
    char[] suit=new char [5];
    int[] rank=new int[5];
    int[] value=new int [5];
    int lowestvalue;
    int binary;
    int fifteen;
    boolean[] run=new boolean[14];
    boolean midthree=true;
    int cardsinrange=0;
    int pointsfromrun;
    for (int k=0;k<14;k++){
      run[k]=false;
    } 
    for(int k=0;k<5;k++){
      hand[k]=cards[k];
      suit[k]=hand[k].findSuit();
      rank[k]=cards[k].findRank();
      value[k]=rank[k];
      if(value[k]>10){
        value[k]=10;
      }         
    }
    for (int i=0;i<5;i++){           //pairs
      for(int j=i+1;j<5;j++){
        if (rank[i]==rank[j]){
          score=score+2;
        }
      }
    } 
    for (int i=0;i<4;i++){       //knobs    
      if((rank[i]==11)&&(suit[i]==suit[4])){
        score++;
      }
    } 
    if ((suit[0]==suit[1])&&(suit[1]==suit[2])&&(suit[2]==suit[3])){ //flush
      if (!crib){
        score=score+4;
        if(suit[4]==suit[3]){
          score=score+1;
        }
      }
      else if(suit[4]==suit[3]){
        score=score+5;
      }
    }
    for(int i=0;i<5;i++){
      run[(rank[i])]=true;
    }
    int valuespresent=0;
    lowestvalue=0;
    for(int j=1;j<14;j++){
      if(run[j]){
        valuespresent++;
      }
    }
    midthree=true;
    if(valuespresent==5){
      while(!run[lowestvalue]){
        lowestvalue++;
      }
      if (run[lowestvalue]&&run[lowestvalue+1]&&run[lowestvalue+2]&&run[lowestvalue+3]&&run[lowestvalue+4]){ //five card run
        score=score+5;
      } 
      else {
        
        if(run[lowestvalue]&&run[lowestvalue+1]&&run[lowestvalue+2]&&run[lowestvalue+3]){
          midthree=false;
          score=score+4; //lowest four cards are a run
        }
        
        else { 
          if(run[lowestvalue]&&run[lowestvalue+1]&&run[lowestvalue+2]){
            score=score+3; //lowest three cards are a run
          } 
        } 
        lowestvalue++;
        while(!run[lowestvalue]){
          lowestvalue++;
        }
        
        if(run[lowestvalue]&&run[lowestvalue+1]&&run[lowestvalue+2]&&run[lowestvalue+3]){
          score=score+4; //highest four cards are a run
        }
        else { 
          if((run[lowestvalue]&&run[lowestvalue+1]&&run[lowestvalue+2])&&(midthree)){
            score=score+3; //middle three cards are a run
          } 
          lowestvalue++;
          while(!run[lowestvalue]){
            lowestvalue++;
          }
          if(run[lowestvalue]&&run[lowestvalue+1]&&run[lowestvalue+2]){
            score=score+3; //highest three cards are a run
          }
        }                                    
      }
    } 
    if(valuespresent==4){                               
      while(!run[lowestvalue]){
        lowestvalue++;
      }
      if(run[lowestvalue]&&run[lowestvalue+1]&&run[lowestvalue+2]&&run[lowestvalue+3]){ //double 4 card run
        score=score+8;
      }
      else {
        cardsinrange=0;
        if(run[lowestvalue]&&run[lowestvalue+1]&&run[lowestvalue+2]){
          for(int m=lowestvalue;m<lowestvalue+3;m++){
            for(int n=0;n<5;n++){
              if(rank[n]==m){
                cardsinrange++;
              }
            }
          }
          if(cardsinrange==4){ //double 3 card run lower than remaining card
            score=score+6;
          }
          else{
            score=score+3; // 3 card run lower than a pair
          }
        } 
        lowestvalue++;
        while(!run[lowestvalue]){
          lowestvalue++;
        }
        if(run[lowestvalue]&&run[lowestvalue+1]&&run[lowestvalue+2]){
          for(int m=lowestvalue;m<lowestvalue+3;m++){
            for(int n=0;n<5;n++){
              if(rank[n]==m){
                cardsinrange++;
              }
            }
          }
          if(cardsinrange==4){ //double three card run for 6 higher than other card
            score=score+6;
          }
          else{ //3 card run higher than a pair
            score=score+3;
          }
        }                                         
      } 
    }                                
    if(valuespresent==3){
      while(!run[lowestvalue]){
        lowestvalue++;
      }
      if(run[lowestvalue]&&run[lowestvalue+1]&&run[lowestvalue+2]){
        pointsfromrun=3;
        for(int t=lowestvalue;t<lowestvalue+3;t++){
          cardsinrange=0;
          for(int n=0;n<5;n++){
            if(rank[n]==t){
              cardsinrange=cardsinrange+1;
            }
          }
          pointsfromrun=pointsfromrun*cardsinrange; //basically shows whether a double/double run or a triple run
        }
        score=score+pointsfromrun;
      } 
    }  
    for(int i=0;i<32;i++){ //fifteens
      binary=i;
      fifteen=0;
      if (binary>15){
        binary=binary-16;
        fifteen=fifteen+value[0];
      }
      if (binary>7){
        binary=binary-8;
        fifteen=fifteen+value[1];
      }
      if (binary>3){
        binary=binary-4;
        fifteen=fifteen+value[2];
      }
      if (binary>1){
        binary=binary-2;
        fifteen=fifteen+value[3];
      }
      if (binary>0){
        binary=binary-1;
        fifteen=fifteen+value[4];
      }
      if (fifteen==15){
        score=score+2;
      }
    } 
    return score;
  }
  public static int scorechecker (ArrayList<Card> cardsInPlay,int count){
    int cardsplayed=cardsInPlay.size();
    boolean [] runs= new boolean [17];
    boolean run=false;
    int lowestvalue;
    int score=0;
    int cardvalue=cardsInPlay.get(cardsplayed-1).findValue();
    if (count==15-cardvalue){
      score=score+2;
    }
    if (count==31-cardvalue){
      score++;
    }
    if ((cardsplayed!=1)&&(cardsInPlay.get(cardsplayed-1).findRank()==cardsInPlay.get(cardsplayed-2).findRank())){
      score=score+2;
      if((cardsplayed!=2)&&(cardsInPlay.get(cardsplayed-1).findRank()==cardsInPlay.get(cardsplayed-3).findRank())){
        score=score+4;
        if((cardsplayed!=3)&&(cardsInPlay.get(cardsplayed-1).findRank()==cardsInPlay.get(cardsplayed-4).findRank())){
          score=score+6;
        }
      }
    }
    for (int j=cardsplayed; 2<j;j--){
      run=false;
      for (int k=0;k<14;k++){ //why is it 17?
        runs[k]=false;
      }
      for (int k=cardsplayed-j; k<cardsplayed;k++){
        runs[cardsInPlay.get(k).findRank()]=true;          
      }
      run=true;
      lowestvalue=0;
      while(!runs[lowestvalue]){
        lowestvalue++;
      }
      for(int k=lowestvalue; k<lowestvalue+j; k++){
        if(!runs[k]){
          run=false;
        }
      }
      if(run){
        score=score+j;
        j=0;
      }
    }
    return (score);
  }
  public static Card pegging(boolean[] cardsInHand, Card[] hand, ArrayList<Card> cardsInPlay, int count){
    Card cardToPlay=new Card (0, (char)'x');
    int score=0;
    int mingain=-100;
    int responsescore=0;
    int tiebreaker;
    int besttiebreaker=100;
    int maxresponsescore=-100;
    int[] handValues=new int[4];
    for(int i=0;i<4;i++){
      handValues[i]=hand[i].findValue();
    }
    for(int i=0;i<4;i++){
      if(count+handValues[i]<32){
        if(cardsInHand[i]){
          cardsInPlay.add(hand[i]);
          score=scorechecker(cardsInPlay, count);
          count=count+handValues[i];
          int responsevalue;
          maxresponsescore=-100;
          tiebreaker=0;
          for(int response=1;response<14;response++){
            responsevalue=response;
            cardsInPlay.add(new Card(response,(char)'\u2764')); //hearts
            if(10<responsevalue){
              responsevalue=10;
            }
            if(31<count+responsevalue){
              responsescore=-1;
            }
            else{
              responsescore=scorechecker(cardsInPlay, count);
            }
            cardsInPlay.remove(cardsInPlay.size()-1);
            tiebreaker=tiebreaker+responsescore;
            if(responsescore>maxresponsescore){
              maxresponsescore=responsescore;
            }
            
          }
          cardsInPlay.remove(cardsInPlay.size()-1);
          count=count-handValues[i];
          if((mingain<score-maxresponsescore)||((mingain==score-maxresponsescore)&&(tiebreaker<besttiebreaker))){
            mingain=score-maxresponsescore;
            besttiebreaker=tiebreaker;
            cardToPlay=hand[i];
          }
        }
      }
      
    }
    return cardToPlay;
  }
  
