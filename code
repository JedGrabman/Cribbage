    falseunsuitedthrows[6][12]=614; 
    falseunsuitedthrows[7][7]=81; 
    falseunsuitedthrows[7][8]=89; 
    falseunsuitedthrows[7][9]=520; 
    falseunsuitedthrows[7][10]=638; 
    falseunsuitedthrows[7][11]=402; 
    falseunsuitedthrows[7][12]=829; 
    falseunsuitedthrows[8][8]=148; 
    falseunsuitedthrows[8][9]=469; 
    falseunsuitedthrows[8][10]=249; 
    falseunsuitedthrows[8][11]=566; 
    falseunsuitedthrows[8][12]=790; 
    falseunsuitedthrows[9][9]=169; 
    falseunsuitedthrows[9][10]=191; 
    falseunsuitedthrows[9][11]=329; 
    falseunsuitedthrows[9][12]=851; 
    falseunsuitedthrows[10][10]=98; 
    falseunsuitedthrows[10][11]=267; 
    falseunsuitedthrows[10][12]=780; 
    falseunsuitedthrows[11][11]=91; 
    falseunsuitedthrows[11][12]=319; 
    falseunsuitedthrows[12][12]=167;
    double totalhand;
    double totalcrib;
    double handscore=0;
    double cribscore=0;
    Card dummy;
    int cribmultiplier;
    int thrown1=0;
    int thrown2=0;
    double totalscore=0;
    double maxtotalscore=-1000000;
    boolean deal;
    
    for (int i=0; i<6; i++){
      cards[i]=hand[i];
    }
    deal=dealer;
    
    //choosing what cards to keep and thus what to throw 
    maxtotalscore=-10000000; 
    for (int a=0;a<6;a++){
      kept[0]=cards[a];
      for(int b=a+1;b<6;b++){
        kept[1]=cards[b];
        for(int c=b+1;c<6;c++){
          kept[2]=cards[c];
          for(int d=c+1;d<6;d++){
            kept[3]=cards[d];
            totalscore=0;
            totalhand=0;
            totalcrib=0;
            for (int crib2=0;crib2<6;crib2++){
              if((cards[crib2]!=kept[0])&&(cards[crib2]!=kept[1])&&(cards[crib2]!=kept[2])&&(cards[crib2]!=kept[3])){
                crib[2]=cards[crib2];
                for (int crib3=crib2+1;crib3<6;crib3++){
                  if((cards[crib3]!=kept[0])&&(cards[crib3]!=kept[1])&&(cards[crib3]!=kept[2])&&(cards[crib3]!=kept[3])){
                    crib[3]=cards[crib3];
                    for(int n=1;n<14;n++){
                      crib[0]=new Card(n,(char)'\u2764');
                      for(int m=1;m<14;m++){
                        crib[1]=new Card(m,(char)'\u2764');
                        for(int cutValue=0;cutValue<52;cutValue++){
                          Card cut=new Card(0,(char)'x');
                          if(cutValue<13){
                            cut=new Card(cutValue%13,(char)'\u2764');
                          }
                          if((cutValue>12)&&(cutValue<26)){
                            cut=new Card(cutValue%13,(char)'\u2666');
                          }
                          if((cutValue>25)&&(cutValue<39)){
                            cut=new Card(cutValue%13,(char)'\u2663');
                          }
                          if(cutValue>39){
                            cut=new Card(cutValue%13,(char)'\u2660');
                          }
                          if (cut.findRank()==0){
                            cut=new Card(13, cut.findSuit());
                          }
                          if((cut!=cards[0])&&(cut!=cards[1])&&(cut!=cards[2])&&(cut!=cards[3])&&(cut!=cards[4])&&(cut!=cards[5])){
                            dummy=new Card(crib[0].findRank(), (char)'\u2663'); 
                            kept[4]=cut;
                            crib[4]=cut;
                            Card [] dummyCrib=new Card[5];
                            for(int i=0; i<5; i++){
                              dummyCrib[i]=crib[i];
                            }
                            dummyCrib[0]=dummy;
                            if(deal){
                              cribmultiplier=1;
                              handscore=(falsesuitedthrows[n][m]+falseunsuitedthrows[n][m])*score(kept, false); //multiplied to keep hand and crib importance balanced
                              cribscore=falsesuitedthrows[n][m]*cribmultiplier*score(crib,true)+falseunsuitedthrows[n][m]*cribmultiplier*score(dummyCrib,true);
                            }
                            if(!deal){
                              cribmultiplier=-1;
                              handscore=(truesuitedthrows[n][m]+trueunsuitedthrows[n][m])*score(kept, false);
                              cribscore=truesuitedthrows[n][m]*cribmultiplier*score(crib,true)+trueunsuitedthrows[n][m]*cribmultiplier*score(dummyCrib,true);
                            }
                            totalhand=totalhand+handscore;
                            totalcrib=totalcrib+cribscore;
                            totalscore=totalscore+handscore+cribscore;
                          }
                        }
                      }
                    }
                    if (totalscore>maxtotalscore){
                      maxtotalscore=totalscore;
                      for(int j=0;j<4;j++){
                        best[j]=kept[j];
                        worst[j]=crib[j];
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
    thrown1=0;
    thrown2=0;
    while ((cards[thrown1]==best[0])||(cards[thrown1]==best[1])||(cards[thrown1]==best[2])||(cards[thrown1]==best[3])){
      thrown1++;
    }
    thrown2=thrown1+1;
    while ((cards[thrown2]==best[0])||(cards[thrown2]==best[1])||(cards[thrown2]==best[2])||(cards[thrown2]==best[3])){
      thrown2++;
    }
    int[] thrown=new int[2];
    thrown[0]=thrown1;
    thrown[1]=thrown2;
    return thrown;
  }
